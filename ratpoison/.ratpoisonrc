escape F13
bind c exec sakura

bind Next exec amixer -q set Master 5- unmute
bind Prior exec amixer -q set Master 5+ unmute
bind m exec amixer -q set Master toggle

unbind k

bind j focusdown
bind h focusleft
bind k focusup
bind l focusright
bind J exchangedown
bind H exchangeleft
bind K exchangeup
bind L exchangeright

bind Escape exec obshutdown -c /home/brandon/.config/obshut/exit.rc

set winname class
set border 0
set padding 0 25 0 0
set bgcolor #333333
set fgcolor #CCCCCC
set barborder 0
